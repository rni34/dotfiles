{
  "suggest.detailMaxLength": 300,
  "python.jediEnabled": false,
  "python.linting.pylintArgs": [
    "--disable=C0103,C0330,W0621,R0912,R0914,R0915,C0115,C0116,C0303,C0326,C0114"
  ],
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": ["--line-length", "95"],
  "languageserver": {
  },
  "vetur.trace.server": "verbose",
  "vetur.format.styleInitialIndent": true,
  "vetur.format.scriptInitialIndent": true,
  "vetur.completion.scaffoldSnippetSources.vetur": "V",
  "eslint.filetypes": ["vue", "js"],
  "snippets.extends": {
    "vue": ["javascript"]
  }
}
